<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pritjoshi.prit_project1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/jaguaricon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/jaguaricon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProductDisplayActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".CheckoutActivity"></activity>
    </application>

</manifest>